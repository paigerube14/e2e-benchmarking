{
    "elasticsearch": {
      "ripsaw-kube-burner": [
        {
          "filter": {
            "metricName.keyword": "kubeletCPU"
          },
          "aggregations": {
            "value": [
              "max"
            ]
          }
        },
        {
            "filter": {
              "metricName.keyword": "kubeletMemory"
            },
            "aggregations": {
              "value": [
                "max"
              ]
            }
          }
      ]
    }
  }
  